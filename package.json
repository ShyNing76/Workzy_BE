{
  "name": "base",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "dev": "babel-node src/config/swagger-auto.js && nodemon --exec babel-node src/server.js",
    "sync": "babel-node src/config/connection.js",
    "test": "mocha --require @babel/register \"src/tests/**/*.js\"",
    "kill": "taskkill /F /IM node.exe",
    "test:email": "babel-node src/tests/email.js",
    "build": "npx babel src --out-dir dist",
    "swagger": "npx babel-node src/config/swagger-auto.js",
    "copy": "cp src/config/config.cjs dist/config/config.cjs",
    "copy:swagger": "cp src/config/swagger-output.json dist/config/swagger-output.json",
    "copy:env": "cp .env dist/.env",
    "start:run": "cd dist && node server.js",
    "start": "npm run build && npm run swagger && npm run copy && npm run copy:swagger && npm run start:run",
    "start:dev": "npm run build && npm run swagger && npm run copy && npm run copy:swagger && npm run copy:env && npm run start:run"
  },
  "type": "commonjs",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@paypal/checkout-server-sdk": "^1.0.3",
    "axios": "^1.7.7",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "cron": "^3.1.7",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "firebase": "^10.14.0",
    "googleapis": "^144.0.0",
    "http-errors": "^2.0.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.15",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "paypal-rest-sdk": "^1.8.1",
    "pg": "^8.13.0",
    "pg-hstore": "^2.3.4",
    "process": "^0.11.10",
    "sequelize": "^6.37.3",
    "sequelize-cli": "^6.2.0",
    "supertest": "^7.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^8.3.2",
    "winston": "^3.15.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.25.7",
    "@babel/core": "^7.25.2",
    "@babel/node": "^7.25.7",
    "@babel/preset-env": "^7.25.4",
    "@babel/register": "^7.24.6",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "mocha": "^10.7.3",
    "nodemon": "^2.0.7",
    "sinon": "^19.0.2",
    "sinon-chai": "^3.6.0",
    "swagger-autogen": "^2.23.7"
  }
}
